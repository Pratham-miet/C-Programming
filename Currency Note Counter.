#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int amount,count;
    scanf("%d", &amount);
    count = amount/500;
    if(count>0){
        printf("500: %d\n", count);
        amount=amount%500;
    }
    count = amount/100;
    if(count>0){
        printf("100: %d\n", count);
        amount=amount%100;
    }
    count = amount/50;
    if(count>0){
        printf("50: %d\n", count);
        amount=amount%50;
    }
    count = amount/20;
    if(count>0){
        printf("20: %d\n", count);
        amount=amount%20;
    }
    count = amount/10;
    if(count>0){
        printf("10: %d\n", count);
        amount=amount%10;
    }
    count = amount/5;
    if(count>0){
        printf("5: %d\n", count);
        amount=amount%5;
    }
    count = amount/2;
    if(count>0){
        printf("2: %d\n", count);
        amount=amount%2;
    }
    count = amount/1;
    if(count>0){
        printf("1: %d\n", count);
        amount=amount%1;
    }
    
    return 0;
}
