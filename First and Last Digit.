#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,First,Last;
    scanf("%d", &n);
    if(n>=0 && n<=9)
        First = Last = n;
    else{
         Last = n%10;
         n = n/10;
         while(n!= 0){
             First = n%10;
             n = n/10;
         }    
    }
        
    printf("%d %d", First,Last);
    return 0;
}
