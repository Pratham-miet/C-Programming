#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>
int main() {
    int countu=0;
    char str[1001];
    scanf("%[^\n]", str);
    for(int i=0; str[i]!='\0'; i++){
        
        if(!isspace(str[i]) && (i == 0 || isspace(str[i - 1]))){
            countu++;
        }   
    }
    printf("%d ", countu);
    return 0;
}
