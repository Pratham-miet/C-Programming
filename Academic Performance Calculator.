#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
   
    float sub1,sub2,sub3,sub4,sub5,total,average,percentage;
    scanf("%f %f %f %f %f" ,&sub1,&sub2,&sub3,&sub4,&sub5);
    total = sub1+sub2+sub3+sub4+sub5;
    average = total/5;
    percentage = (total/500)*100;
    printf("%.0f %.2f %.2f",total,average,percentage);
    
    return 0;
}
