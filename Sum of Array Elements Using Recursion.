#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int sumArray(int arr[], int num){
    if(num==0){
        return 0;
    }
        return arr[num-1]+sumArray(arr,num-1);
}
int main() {
    int n;
    scanf("%d", &n);
    int arr[n];
    for( int i=0; i<n; i++){
        scanf("%d", &arr[i]);
    }
    printf("%d", sumArray(arr,n));
    return 0;
}
