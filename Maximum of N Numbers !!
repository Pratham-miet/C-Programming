#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int i,N,num,max=0;
    scanf("%d", &N);
    scanf("%d", &num);
    max = num;
    for(i=1; i<=N; i+=1){
        scanf("%d", &num);
        if(num > max)
            max = num;
    }
    printf("%d", max);

    return 0;
}
