#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int reverseNumber(int num){
    int count=0,temp;
    temp=num;
    while(temp!=0){
        temp/=10;
        count++;
        
    }
    double p=pow(10,count-1);
    if(num==0){
        return 0;
    }
    else 
        return num%10*p+reverseNumber(num/10);
}
int main() {
    int num;
    scanf("%d", &num);
    printf("%d", reverseNumber(num));
    return 0;
}
